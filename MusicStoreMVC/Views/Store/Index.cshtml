@model IEnumerable<MusicStoreMVC.Models.Genre>

@{
    ViewBag.Title = "Index";
}

<h3>Browse Genres</h3>
<p>Select from @Model.Count() genres: </p>
<ul>
    @foreach (var genre in Model)
    {
        <li>
            <a href="/Store/Browse?genre=@genre.Name">@genre.Name</a>
        </li>
    }
</ul>

